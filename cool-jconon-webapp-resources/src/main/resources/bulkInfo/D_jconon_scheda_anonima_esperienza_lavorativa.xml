<bulkInfo xmlns="http://www.cnr.it/schema/BulkInfo_v2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	cmisQueryName="jconon_scheda_anonima:esperienza_lavorativa"
	cmisTypeName="D:jconon_scheda_anonima:esperienza_lavorativa"
	cmisExtendsName="D:jconon_scheda_anonima:document" 
	shortDescription="Esperienza Lavorativa"
	xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">
	<cmisImplementsName join="true"
		name="P:jconon_scheda_anonima:common_contratto" />
	<fieldProperty 
		name="common_contratto_data_inizio"> 
		<jsonlabel default="Data inizio attività" key="label.jconon_scheda_anonima.common_contratto_data_inizio" />
	</fieldProperty>
	<fieldProperty 
		name="common_contratto_data_fine">
		<jsonlabel default="Data fine attività" key="label.jconon_scheda_anonima.common_contratto_data_fine" />
	</fieldProperty>
	<fieldProperty
			name="common_contratto_denominazione_ente">
		<jsonlabel default="Svolta presso:(Indicare Denominazione ed ambito Pubblio o Privato)" key="label.jconon_scheda_anonima.esperienza_lavorativa_denominazione_ente" />
	</fieldProperty>
	<fieldProperty 
		name="common_contratto_descrizione_attivita" >
		<jsonlabel default="Descrizione esperienza lavorativa" key="label.jconon_scheda_anonima.esperienza_lavorativa_descrizione_attivita" />
		<jsonvalidator required="true" maxlengthAlfresco="1000"/>
	</fieldProperty>
	<formFieldProperty 
		name="common_contratto_data_inizio"/> 		
	<formFieldProperty 
		name="common_contratto_incorso"/> 		
	<formFieldProperty 
		name="common_contratto_data_fine"/> 		
	<formFieldProperty 
		name="common_contratto_denominazione_ente"/>
	<formFieldProperty 
		name="common_contratto_descrizione_attivita"/> 		

	<columnFieldProperty 
		name="common_contratto_data_inizio"/> 		
	<columnFieldProperty 
		name="common_contratto_incorso"/> 		
	<columnFieldProperty 
		name="common_contratto_data_fine"/> 		
	<columnFieldProperty 
		name="common_contratto_denominazione_ente"/> 		
	<columnFieldProperty 
		name="common_contratto_descrizione_attivita"/> 		

</bulkInfo>